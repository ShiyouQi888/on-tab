{
  "manifest_version": 3,
  "name": "__MSG_appName__",
  "version": "1.0.0",
  "version_name": "1.0.0 (Stable)",
  "description": "__MSG_appDesc__",
  "author": "ShiyouQi888",
  "homepage_url": "https://on-tab.vercel.app",
  "default_locale": "zh_CN",
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "tabs",
    "notifications",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {},
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.ts",
    "type": "module"
  }
}
